{
  "title": "Event",
  "type": "object",
  "properties": {
    "data": {
      "title": "EventData",
      "type": "object",
      "properties": {
        "identifiers": {
          "description": "Known identifiers for an event",
          "type": "object",
          "uniqueItems": true,
          "minProperties": 1,
          "additionalProperties": {
            "type": "array",
            "minItems": 1,
            "uniqueItems": true
          }
        },
        "entries": {
          "description": "Entries for an event",
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": ["type"]
          },
          "minItems": 1
        },
        "location": {
          "description": "A geographical coordinate",
          "type": "object",
          "oneOf": [
            {
              "properties": {
                "latitude": {
                  "type": "number",
                  "minimum": -90,
                  "maximum": 90
                },
                "longitude": {
                  "type": "number",
                  "minimum": -180,
                  "maximum": 180
                }
              },
              "required": [
                "latitude",
                "longitude"
              ],
              "additionalProperties": false
            },
            {
              "properties": {
                "asset": {
                  "type": "string"
                }
              },
              "required": [
                "asset"
              ],
              "additionalProperties": false
            }
          ]
        }
      },
      "required": ["entries"],
      "additionalProperties": false
    }
  }
}
